{
  "name": "simple-mvc-example",
  "version": "0.0.0",
  "engines": {
    "node": "10.12.0",
    "npm": "6.4.1"
  },
  "description": "A simple Express MVC using only static files",
  "main": "./server/app.js",
  "scripts": {
    "build": "babel ./client --out-file ./hosted/bundle.js",
    "watch": "babel --watch ./client --out-file ./hosted/bundle.js",
    "nodemon": "nodemon --watch ./server ./server/app.js",
    "start": "node ./server/app.js",
    "pretest": "eslint ./server --fix",
    "test": "echo \"tests complete\"",
    "debug": "node --inspect ./server/app.js"
  },
  "author": "Ian Effendi",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-env": "^1.1.8",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "nodemon": "^1.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rimij405/Simple-MVC-Example.git"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "gulp": "^4.0.0",
    "mongoose": "^4.6.3",
    "serve-favicon": "^2.3.0"
  }
}
