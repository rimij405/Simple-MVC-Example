<html lang="en">
<head>
  <meta charset="utf-8"/>
  <title>{{title}}</title>
  <link rel="stylesheet" type="text/css" href="/hosted/style.css" />
  <script type="application/javascript" src="/hosted/bundle.js" ></script>
</head>
<body>
  <h1>Welcome to page1</h1>
  
  <!-- links or URLs are always GET resources as they are getting (retrieving) a resource -->
  <a href="/">Home</a>
  <a href="/page2">Page 2</a>
  <a href="/page3">Page 3</a>
  <a href="/page4">Page 4</a>
  <a href="/getCatName">Show Cat Name</a>
  
  {{#if cats}}
    <ul>
      {{#each cats}}
        <li> {{this.name}} has {{this.bedsOwned}} beds</li>
      {{/each}}
    </ul>
  {{else}}
    <p>No cats to display.</p>
  {{/if}}

</body>
</html>